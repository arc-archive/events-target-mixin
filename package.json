{
  "name": "@advanced-rest-client/events-target-mixin",
  "description": "Mixin that support event targets retargeting so the element listens on a set node instead of default one",
  "version": "3.2.5",
  "license": "Apache-2.0",
  "main": "index.js",
  "module": "index.js",
  "keywords": [
    "web-components",
    "events",
    "mixin"
  ],
  "authors": [
    "Pawel Psztyc"
  ],
  "contributors": [
    "The Advanced REST client authors <arc@mulesoft.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/events-target-mixin.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/events-target-mixin/issues",
    "email": "arc@mulesoft.com"
  },
  "dependencies": {
    "@open-wc/dedupe-mixin": "^1.3.0"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "^2.5.33",
    "@web/test-runner": "^0.13.18",
    "@web/test-runner-playwright": "^0.8.8",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "lit-element": "^2.5.1",
    "lit-html": "^1.4.1",
    "sinon": "^11.1.2",
    "typescript": "^4.4.3",
    "typescript-lit-html-plugin": "^0.9.0"
  },
  "scripts": {
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint",
    "test": "web-test-runner test/**/*.test.js --coverage --node-resolve --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner test/**/*.test.js --node-resolve --watch --playwright --browsers chromium"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  }
}
